generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String   @id @default(uuid())
  email    String   @unique
  password String?  // null si viene de Google
  name     String
  picture  String?
  provider String   @default("local")
  createdAt DateTime @default(now())
}
